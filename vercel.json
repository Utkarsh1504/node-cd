{
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src":"build/**",
      "dest":"@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/products",
      "dest": "index.js"
    },
    {
      "src":"/products/(.*)",
      "dest":"index.js"
    },
    {
      "src":"/",
      "dest":"build/index.html"
    },
    {
      "src":"/(.+)",
      "dest":"build/$1"
    }
  ]
}